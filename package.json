{
  "author": "Andrew Reida",
  "name": "spacesynter",
  "version": "0.0.0",
  "description": "An app for quickly launching other apps",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/areida/spacesynter.git"
  },
  "scripts": {
    "deploy:app": "aws s3 cp dist/`git rev-parse --abbrev-ref HEAD`-`git rev-parse HEAD | head -c 10`.zip s3://spacesynter/app.zip",
    "deploy:cookbooks": "aws s3 cp dist/cookbooks.zip s3://spacesynter/cookbooks.zip",
    "dist:app": "APP_ENV=production NODE_ENV=production webpack -p",
    "dist:cookbooks": "mkdir -p dist && zip -r dist/cookbooks cookbooks",
    "start:dev": "pm2 start server/dev.js -n spacesynter:dev",
    "start:app": "pm2 start server/index.js -n spacesynter:app --watch",
    "prestart": "npm run clean",
    "clean": "rm -rf build",
    "deploy": "npm run deploy:app && npm run deploy:cookbooks",
    "dist": "npm run dist:app && npm run dist:cookbooks && ./dist.sh && npm run deploy",
    "start": "npm run start:app && npm run start:dev && ./start.sh"
  },
  "devDependencies": {
    "babel-loader": "5.3.2",
    "css-loader": "0.15.6",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.6.0",
    "json-loader": "0.5.2",
    "jshint-loader": "0.8.3",
    "jsx-loader": "0.13.2",
    "jsxhint-loader": "0.2.0",
    "raw-loader": "0.5.1",
    "react-hot-loader": "1.2.8",
    "react-tools": "0.13.3",
    "sass-loader": "1.0.3",
    "style-loader": "0.12.3",
    "url-loader": "0.5.6",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.10.1",
    "webpack-error-notification": "0.1.4"
  },
  "dependencies": {
    "blueimp-tmpl": "2.5.5",
    "body-parser": "1.13.3",
    "classnames": "2.1.3",
    "connect-redis": "2.4.1",
    "cookie-parser": "1.3.5",
    "express": "4.13.2",
    "express-http-proxy": "^0.6.0",
    "express-session": "1.11.3",
    "fluxxor": "1.7.0",
    "immutable": "3.7.4",
    "lodash": "3.10.0",
    "minimist": "1.1.2",
    "moment": "2.10.6",
    "mongodb": "2.0.39",
    "mongoose": "4.1.0",
    "node-resque": "0.16.4",
    "openport": "0.0.4",
    "pm2": "0.14.5",
    "q": "1.4.1",
    "react": "0.13.3",
    "react-router": "0.13.3",
    "request": "2.60.0",
    "store": "1.3.17",
    "synapse-common": "1.8.2",
    "synfrastructure": "1.0.0",
    "then-redis": "1.3.0"
  }
}
